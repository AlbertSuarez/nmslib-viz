#!/usr/bin/env python
import argparse
import nmslib_viz

from nmslib_viz.config import HELP_NMSLIB_INDEX_FILE_PATH


parser = argparse.ArgumentParser()
parser.add_argument('nmslib_index_file_path', type=str, help=HELP_NMSLIB_INDEX_FILE_PATH)
args = parser.parse_args()
nmslib_viz.view(args.nmslib_index_file_path)
